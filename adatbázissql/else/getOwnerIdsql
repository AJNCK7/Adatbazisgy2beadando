create or replace procedure sf_getownerid
(
    p_surname in owners.surname%type
)
return int
deterministic

as
    temp int;
    temp_cnt int;
begin
    select count(*) into temp_cnt FROM owners where nev = p_nev;
    if temp_cnt = 0 then
        return null;
    else
        SELECT id into temp from owners where surName = p_surname;
    end if;
    
    return temp;
end sf_getownerid;